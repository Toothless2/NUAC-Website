<tr>
    <td title="Archer"><%= record.name %> 
        <% if can_edit_record(record) %>
            <%= link_to 'Edit', edit_record_path(record) %>
            <%= link_to 'Destroy', record, method: :delete, data: { confirm: 'Are you sure?' } %>
        <%end%>
    </td>
    <td title="Score"><%= record.score %></td>
    <td title="Round"><%= record.round %></td>
    <td><%= record.bowstyle %></td>
    <td><%= record.achived_at %></td>
    <td><%= record.academic_year_string %></td>
    <td><%= record.gender.to_s.titlecase %></td>
</tr>